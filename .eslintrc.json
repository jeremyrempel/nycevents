{
  "plugins": [
    "react"
  ],
  "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "react/prop-types": "off",
        "no-console": "off"
    },
    "globals": {
      "fetch": false,
      "require": false,
      "navigator": false
    }
}
